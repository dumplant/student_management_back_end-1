<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.student_management.demo.mapper.mysql.student.StudentMapper">
    <select id="selectStuffByNum" resultType="com.student_management.demo.mapper.dataobject.staff.StaffDO">
        SELECT id, num, name, job, sex, level, email,passwd FROM stuff WHERE num = #{num}
    </select>


</mapper>