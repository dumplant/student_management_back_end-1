<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.student_management.demo.mapper.mysql.student.StudentMapper">
    <!--  通过学号查找学生信息  -->
    <select id="findByNum" resultType="com.student_management.demo.mapper.dataobject.student">
        SELECT id, num, name, sex, year_id, major_id, class_id, email, status, passwd FROM stu_info WHERE num = #{num}
    </select>


</mapper>